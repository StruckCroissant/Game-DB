{
  "name": "game-db",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test:component:vitest": "vitest --environment jsdom --root src/",
    "test:component": "npm run test:component:vitest",
    "test:application:vitest": "npx vitest run ./test/specs",
    "test:application": "npm run test:application:vitest",
    "test:e2e:cypress": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:cypress:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "test:e2e": "npm run test:e2e:cypress"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@vee-validate/zod": "^4.11.8",
    "@vueuse/core": "^9.11.1",
    "lodash": "^4.17.21",
    "pinia": "^2.0.28",
    "vee-validate": "^4.11.8",
    "vue": "^3.2.45",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.1.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.200",
    "@types/node": "^18.16.9",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.0.12"
  }
}
