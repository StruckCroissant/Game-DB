{
  "name": "game-db",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:ci": "eslint . --ignore-path .gitignore --max-warnings 0 --no-fix",
    "test:unit": "vue-cli-service test:unit --environment=happy-dom",
    "test:unit:watch": "vue-cli-service test:unit --watch --environment=happy-dom",
    "test:unit:coverage": "nyc -t ./.nyc_output/unit --reporter=lcov vue-cli-service test:unit",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@vee-validate/zod": "^4.11.8",
    "@vueuse/core": "^9.11.1",
    "happy-dom": "^13.3.1",
    "lodash": "^4.17.21",
    "pinia": "^2.0.28",
    "vee-validate": "^4.11.8",
    "vue": "^3.2.45",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.1.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.19",
    "@playwright/test": "^1.40.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^8.0.1",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.200",
    "@types/node": "^18.16.9",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "cypress": "^13.6.3",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jest": "^27.0.5",
    "jsdom": "^20.0.3",
    "msw": "^2.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "start-server-and-test": "^1.15.2",
    "ts-jest": "^27.0.4",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-istanbul": "^5.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
